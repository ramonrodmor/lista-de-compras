<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.rodmor.listadecompras">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_icon_carrinho"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_icon_carrinho"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".AdicionarItem"></activity>
        <activity
            android:name=".Lista"
            android:label="@string/title_activity_lista" />
        <activity
            android:name=".Compras"
            android:label="@string/title_activity_compras" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>